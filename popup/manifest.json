{
  "manifest_version": 2,
  "author": "Manaswita Mishra",
  "name": "Pop up",
  "version": "0.1",

  "permissions": [
    "activeTab",
    "tabs",
    "<all_urls>",
    "http://*/",
    "https://*/"
  ],
  "browser_action": {
    "default_icon": "tab-icon.png",
    "default_popup": "popup.html",
    "default_title": "This is a test popup"
  }
}